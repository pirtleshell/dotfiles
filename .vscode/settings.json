{
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [100],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.eol": "\n",
  "terminal.integrated.env.osx": {
    "VISUAL": "/usr/local/bin/code --wait"
  },
  "workbench.editor.wrapTabs": true,
  "codemetrics.nodeconfiguration.CaseClause": 0,
  "codemetrics.nodeconfiguration.ReturnStatement": 0.25,
  "codemetrics.nodeconfiguration.DefaultClause": 0,
  "gitlens.blame.ignoreWhitespace": true,
  "swaggerViewer.previewInBrowser": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "typescript.referencesCodeLens.enabled": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "authorizable",
    "baseload",
    "healthcheck",
    "hotspot",
    "interactor",
    "interactors",
    "kpis",
    "OIDC",
    "Pirtle",
    "postgres",
    "structs",
    "upsert",
    "upserting",
    "webauthn"
  ],
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "redhat.telemetry.enabled": false,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "swaggerViewer.defaultPort": 10239,
  "docker.showStartPage": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "go.toolsManagement.autoUpdate": true,
  "editor.formatOnSave": true,
  "githubPullRequests.terminalLinksHandler": "github",
  "window.zoomLevel": 1,
  "terminal.integrated.enableMultiLinePasteWarning": false
}
